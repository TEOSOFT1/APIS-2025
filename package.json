{
  "name": "apis-2025",
  "version": "1.0.0",
  "description": "Proyecto de APIs con Node.js, React y MySQL",
  "main": "App.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node App.js",
    "dev": "cross-env NODE_ENV=development nodemon App.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "docs": "swagger-jsdoc -d swaggerDef.js -o swagger.json",
    "clean": "rimraf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@google-cloud/sql": "^5.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.7",
    "dotenv-flow": "^3.3.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.14.0",
    "puppeteer": "^24.4.0",
    "sanitize-html": "^2.15.0",
    "sequelize": "^6.37.6",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "typescript-eslint": "^8.27.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}